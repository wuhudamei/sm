<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mdni.business.dao.material.ProjectIntemTypeDao">

    <resultMap id="BaseResultMap" type="cn.mdni.business.entity.material.ProjectIntemType">
        <id column="id" property="id"/>
        <id column="project_intem_type_name" property="projectIntemTypeName"/>
        <id column="status" property="status"/>

    </resultMap>

    <sql id="Base_Column_List">
        id,project_intem_type_name,`status`
    </sql>

    <select id="getById" parameterType="long" resultMap="BaseResultMap">
        SELECT
          <include refid="Base_Column_List"/>
        FROM
        sm_project_intem_type
        where id = #{id}
    </select>

    <insert id="insert" parameterType="cn.mdni.business.entity.material.ProjectIntemType">
    insert into sm_project_intem_type(
    id,
    project_intem_type_name,
    `status`
    ) values
    (
    #{id},
    #{projectIntemTypeName},
    #{status}
    )
</insert>

    <update id="update" parameterType="cn.mdni.business.entity.material.ProjectIntemType">
        update sm_project_intem_type
        set
        project_intem_type_name = #{projectIntemTypeName},
        `status` = #{status}
        where
        id = #{id}
    </update>



    <select id="getProjectIntemType"  resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from sm_project_intem_type
    </select>

</mapper>
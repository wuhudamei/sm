	 <mvc:view-controller path="/singletag/singletaglist" view-name="orderflow/singleTagList"/>
	 <!--串单管理-项目管理-->
	 <mvc:view-controller path="/singletag/projectManage" view-name="orderflow/projectManage"/>
	 <!---->
	 <!--<mvc:view-controller path="/aaa" view-name=""/>-->
	 <!--选材管理-->
	 <mvc:view-controller path="/business/customercontract/customercontract"
								 view-name="/customercontract/customercontract"/>

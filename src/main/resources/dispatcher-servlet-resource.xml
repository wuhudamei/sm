								 view-name="/material/labelList"/>
	 <!--功能区管理-->
	 <mvc:view-controller path="/domaininfo/list" view-name="domaininfo/list"/>
	 <!--套餐管理-->
	 <mvc:view-controller path="/mealinfo/meallist" view-name="material/mealInfoList"/>
	 
	 <mvc:view-controller path="/orderflow/order/list" view-name="orderflow/order/list"/>


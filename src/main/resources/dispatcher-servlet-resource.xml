	<!--主材变更单打印-->
	<mvc:view-controller path="/finance/print/changelist" view-name="finance/print/changeList"/>
	 <!--基装变更单打印-->
	 <mvc:view-controller path="/finance/print/baselist" view-name="finance/print/baseList"/>
	 <!--收据打印-->
	 <mvc:view-controller path="/finance/print/receiptlist" view-name="finance/print/receiptlist"/>

	 <!--财务管理-->
	 <!--财务配置-->
	 <mvc:view-controller path="/finance/finapaymethod/queryfinapaymethodlist"
								 view-name="finance/finaPaymethod/queryFinaPaymethodAll"/>
	 <!--通用特殊配置-->
	 <mvc:view-controller path="/finance/finaPaymethod/particularConfig"
								 view-name="finance/finaPaymethod/particularConfig"/>

